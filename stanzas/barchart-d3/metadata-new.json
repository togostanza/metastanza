{
  "@context": {
    "stanza": "http://togostanza.org/resource/stanza#"
  },
  "stanza:parameter": [
    {
      "stanza:key": "data_url",
      "stanza:example": "https://sparql-support.dbcls.jp/sparqlist/api/metastanza_multi_data_chart",
      "stanza:description": "Data source URL",
      "stanza:required": true
    },
    {
      "stanza:key": "data_type",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "json",
        "tsv",
        "csv",
        "sparql-results-json"
      ],
      "stanza:example": "json",
      "stanza:description": "Data type",
      "stanza:required": true
    },
    {
      "stanza:key": "misc_custom-css-url",
      "stanza:example": "",
      "stanza:description": "Stylesheet(css file) URL to override current style",
      "stanza:required": false
    },
    {
      "stanza:key": "chart-type",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "stacked",
        "grouped"
      ],
      "stanza:example": "stacked",
      "stanza:description": "Type of barchart",
      "stanza:required": true
    },
    {
      "stanza:key": "axis_x_data-key",
      "stanza:type": "text",
      "stanza:example": "category",
      "stanza:description": "What key in data to use",
      "stanza:required": true
    },
    {
      "stanza:key": "axis_x_show",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show this axis"
    },
    {
      "stanza:key": "axis_x_axis-placement",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "bottom",
        "top"
      ],
      "stanza:example": "bottom",
      "stanza:description": "Axis placement"
    },
    {
      "stanza:key": "axis_x_scale",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "linear",
        "ordinal",
        "log10",
        "time"
      ],
      "stanza:example": "linear",
      "stanza:description": "Scale type"
    },
    {
      "stanza:key": "axis_x_range-min",
      "stanza:type": "number",
      "stanza:example": 0,
      "stanza:description": "Axis range minimum value"
    },
    {
      "stanza:key": "axis_x_range-max",
      "stanza:type": "number",
      "stanza:description": "Axis range maximum value"
    },
    {
      "stanza:key": "axis_x_title",
      "stanza:example": "chromosome",
      "stanza:description": "Axis title"
    },
    {
      "stanza:key": "axis_x_title-padding",
      "stanza:example": 7,
      "stanza:description": "Axis title padding"
    },
    {
      "stanza:key": "axis_x_ticks-interval",
      "stanza:example": 1,
      "stanza:description": "Axis ticks interval"
    },
    {
      "stanza:key": "axis_x_ticks-interval-units",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "none",
        "ms",
        "s",
        "minutes",
        "hours",
        "days",
        "weeks",
        "months",
        "years"
      ],
      "stanza:example": "none",
      "stanza:description": "Axis ticks interval units"
    },
    {
      "stanza:key": "axis_x_labels-format",
      "stanza:type": "text",
      "stanza:example": ".2f",
      "stanza:description": "Axis labels format"
    },
    {
      "stanza:key": "axis_x_gridlines-interval",
      "stanza:example": 1,
      "stanza:type": "number",
      "stanza:description": "Axis gridlines interval"
    },
    {
      "stanza:key": "axis_x_gridlines-interval-units",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "none",
        "ms",
        "s",
        "minutes",
        "hours",
        "days",
        "weeks",
        "months",
        "years"
      ],
      "stanza:example": "none",
      "stanza:description": "Axis gridlines interval units"
    },
    {
      "stanza:key": "axis_y_data-key",
      "stanza:type": "text",
      "stanza:example": "category",
      "stanza:description": "What key in data to use",
      "stanza:required": true
    },
    {
      "stanza:key": "axis_y_show",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show this axis"
    },
    {
      "stanza:key": "axis_y_axis-placement",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "left",
        "right"
      ],
      "stanza:example": "left",
      "stanza:description": "Axis placement"
    },
    {
      "stanza:key": "axis_y_scale",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "linear",
        "ordinal",
        "log10",
        "time"
      ],
      "stanza:example": "linear",
      "stanza:description": "Scale type"
    },
    {
      "stanza:key": "axis_y_range-min",
      "stanza:type": "number",
      "stanza:example": 0,
      "stanza:description": "Axis range minimum value",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_range-max",
      "stanza:type": "number",
      "stanza:description": "Axis range maximum value",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_title",
      "stanza:example": "chromosome",
      "stanza:description": "Axis title",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_title-padding",
      "stanza:example": 7,
      "stanza:description": "Axis title padding",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_ticks-interval",
      "stanza:example": 1,
      "stanza:description": "Axis ticks interval",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_ticks-interval-units",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "none",
        "ms",
        "s",
        "minutes",
        "hours",
        "days",
        "weeks",
        "months",
        "years"
      ],
      "stanza:example": "none",
      "stanza:description": "Axis ticks interval units"
    },
    {
      "stanza:key": "axis_y_labels-format",
      "stanza:type": "text",
      "stanza:example": ".2f",
      "stanza:description": "Axis labels format",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_gridlines-interval",
      "stanza:example": 1,
      "stanza:type": "number",
      "stanza:description": "Axis gridlines interval",
      "stanza:required": false
    },
    {
      "stanza:key": "axis_y_gridlines-interval-units",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "none",
        "ms",
        "s",
        "minutes",
        "hours",
        "days",
        "weeks",
        "months",
        "years"
      ],
      "stanza:example": "none",
      "stanza:description": "Axis gridlines interval units"
    },
    {
      "stanza:key": "data-series_group-by_data-key",
      "stanza:type": "text",
      "stanza:example": "id",
      "stanza:description": "What key in data to use to group points into data series"
    },
    {
      "stanza:key": "data-series_color_data-key",
      "stanza:type": "text",
      "stanza:example": "category",
      "stanza:description": "What key in data to use to color the data series"
    },
    {
      "stanza:key": "datapoints-color_data-key",
      "stanza:type": "text",
      "stanza:example": "category",
      "stanza:description": "What key in data to use to color the data points"
    },
    {
      "stanza:key": "datapoints-color_scale",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "linear",
        "ordinal"
      ],
      "stanza:example": "ordinal",
      "stanza:description": "What key in data to use to color the data series"
    },
    {
      "stanza:key": "tooltips_data-key",
      "stanza:type": "text",
      "stanza:example": "count",
      "stanza:description": "Data key to show tooltips. No tooltips are shown if empty"
    }
  ],
  "stanza:menu-placement": "bottom-right",
  "stanza:style": [
    {
      "stanza:key": "--togostanza_theme_series-0-color",
      "stanza:type": "color",
      "stanza:default": "aquamarine",
      "stanza:description": "Group color 0",
      "new_prop": "aquamarine"
    },
    {
      "stanza:key": "--togostanza_theme_series-1-color",
      "stanza:type": "color",
      "stanza:default": "#3ac9b6",
      "stanza:description": "Group color 1"
    },
    {
      "stanza:key": "--togostanza_theme_series-2-color",
      "stanza:type": "color",
      "stanza:default": "#9ede2f",
      "stanza:description": "Group color 2"
    },
    {
      "stanza:key": "--togostanza_theme_series-3-color",
      "stanza:type": "color",
      "stanza:default": "#F5DA64",
      "stanza:description": "Group color 3"
    },
    {
      "stanza:key": "--togostanza_theme_series-4-color",
      "stanza:type": "color",
      "stanza:default": "#F57F5B",
      "stanza:description": "Group color 4"
    },
    {
      "stanza:key": "--togostanza_theme_series-5-color",
      "stanza:type": "color",
      "stanza:default": "#F75976",
      "stanza:description": "Group color 5"
    },
    {
      "stanza:key": "--togostanza_fonts_font-family",
      "stanza:type": "text",
      "stanza:default": "Helvetica Neue",
      "stanza:description": "Font family"
    },
    {
      "stanza:key": "--togostanza_fonts_font-color",
      "stanza:type": "color",
      "stanza:default": "#4E5059",
      "stanza:description": "Font color"
    },
    {
      "stanza:key": "--togostanza_fonts_font-size-primary",
      "stanza:type": "number",
      "stanza:default": 12,
      "stanza:description": "Primary font size"
    },
    {
      "stanza:key": "--togostanza_fonts_font-size-secondary",
      "stanza:type": "number",
      "stanza:default": 9,
      "stanza:description": "Secondary font size"
    },
    {
      "stanza:key": "--togostanza_outline_width",
      "stanza:type": "number",
      "stanza:default": 300,
      "stanza:description": "Width"
    },
    {
      "stanza:key": "--togostanza_outline_height",
      "stanza:type": "number",
      "stanza:default": 300,
      "stanza:description": "Height"
    },
    {
      "stanza:key": "--togostanza_outline_padding-top",
      "stanza:type": "number",
      "stanza:default": 20,
      "stanza:description": "Padding top"
    },
    {
      "stanza:key": "--togostanza_outline_padding-right",
      "stanza:type": "number",
      "stanza:default": 20,
      "stanza:description": "Padding right"
    },
    {
      "stanza:key": "--togostanza_outline_padding-bottom",
      "stanza:type": "number",
      "stanza:default": 20,
      "stanza:description": "Padding bottom"
    },
    {
      "stanza:key": "--togostanza_outline_padding-left",
      "stanza:type": "number",
      "stanza:default": 20,
      "stanza:description": "Padding left"
    },
    {
      "stanza:key": "--togostanza_border_color",
      "stanza:type": "color",
      "stanza:default": "#4E5059",
      "stanza:description": "Border color"
    },
    {
      "stanza:key": "--togostanza_border_width",
      "stanza:type": "number",
      "stanza:default": 0,
      "stanza:description": "Border width"
    },
    {
      "stanza:key": "--togostanza_axis_x_width",
      "stanza:type": "number",
      "stanza:default": 1,
      "stanza:description": "Axis width"
    },
    {
      "stanza:key": "--togostanza_axis_y_width",
      "stanza:type": "number",
      "stanza:default": 1,
      "stanza:description": "Axis width"
    },
    {
      "stanza:key": "--togostanza_data-series_fixed-color",
      "stanza:type": "color",
      "stanza:default": "#4E5059",
      "stanza:description": "Default solor for data series"
    }
  ],
  "@id": "barchart-d3",
  "stanza:label": "Barchart-D3",
  "stanza:definition": "Barchart-D3 MetaStanza",
