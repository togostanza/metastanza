      "stanza:required": false
    },
    {
      "stanza:key": "error_bars-data_key",
      "stanza:type": "text",
      "stanza:example": "error",
      "stanza:description": "Data key wich includes error data. Error data is array [min, max]. If no such data key, no error bars are shown",
      "stanza:required": false
