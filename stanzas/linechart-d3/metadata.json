      "stanza:required": false
    },
    {
      "stanza:key": "axis-x-gridlines_interval",
      "stanza:type": "single-choice",
      "stanza:choice": [
        "none",
        "century",
        "year",
        "month",
        "day",
        "hour",
        "minute",
        "second",
        "millisecond"
