    {
      "stanza:key": "data_grouping-color-scheme",
      "stanza:type": "single-choice",
      "stanza:example": [
        "Custom-Theme",
        "Virdilis",
        "Jet",
        "Blues",
        "Greens",
        "Reds",
        "Greys",
        "Purples",
        "Oranges"
