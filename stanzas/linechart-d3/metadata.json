      "stanza:example": "none",
      "stanza:description": "Axis grid lines interval",
      "stanza:required": false
    },
    {
      "stanza:key": "axis-x-gridlines_interval_units",
      "stanza:type": "single-choice",
      "stanza:example": "",
      "stanza:description": "Axis grid lines interval units",
      "stanza:required": false
