{
  "@context": {
    "stanza": "http://togostanza.org/resource/stanza#"
  },
  "@id": "linechart-d3",
  "stanza:label": "Linechart-D3",
  "stanza:definition": "Linechart-D3 MetaStanza",
  "stanza:license": "MIT",
  "stanza:author": "DBCLS",
  "stanza:address": "https://github.com/togostanza/metastanza",
