{
  "@context": {
    "stanza": "http://togostanza.org/resource/stanza#"
  },
  "@id": "force-graph",
  "stanza:label": "Force Graph",
  "stanza:definition": "Force Graph MetaStanza",
  "stanza:license": "MIT",
  "stanza:author": "DBCLS",
  "stanza:address": "https://github.com/togostanza/metastanza",
  "stanza:contributor": ["PENQE", "Einishi Tech"],
  "stanza:created": "2021-02-16",
  "stanza:updated": "2021-02-16",
  "stanza:parameter": [
    {
      "stanza:key": "data-url",
      "stanza:example": "https://gist.githubusercontent.com/abkunal/98d35b9b235312e90f3e43c9f7b6932b/raw/d5589ddd53731ae8eec7abd091320df91cdcf5cd/miserables.json",
      "stanza:description": "Data source URL",
      "stanza:required": true
    },
    {
      "stanza:key": "data-type",
      "stanza:type": "single-choice",
      "stanza:choice": ["json", "tsv", "csv", "sparql-results-json"],
      "stanza:example": "json",
      "stanza:description": "Data type",
      "stanza:required": true
    },

    {
      "stanza:key": "width",
      "stanza:type": "number",
      "stanza:example": 600,
      "stanza:description": "Width"
    },
    {
      "stanza:key": "height",
      "stanza:type": "number",
      "stanza:example": 600,
      "stanza:description": "Height"
    },
    {
      "stanza:key": "show-node-labels",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show node labels"
    },
    {
      "stanza:key": "node-label-key",
      "stanza:type": "string",
      "stanza:example": "id",
      "stanza:description": "Data key for node labels"
    },
    {
      "stanza:key": "node-color-key",
      "stanza:type": "string",
      "stanza:example": "group",
      "stanza:description": "Data key for node background color"
    },
    {
      "stanza:key": "show-node-tooltips",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show node tooltips"
    },
    {
      "stanza:key": "node-tooltip-key",
      "stanza:type": "string",
      "stanza:example": "id",
      "stanza:description": "Node tooltip data key"
    },
    {
      "stanza:key": "show-edge-labels",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show edge tooltips"
    },
    {
      "stanza:key": "show-edge-tooltips",
      "stanza:type": "boolean",
      "stanza:example": true,
      "stanza:description": "Show edge tooltips"
    },
    {
      "stanza:key": "edge-tooltip-key",
      "stanza:type": "string",
      "stanza:example": "label",
      "stanza:description": "Edge tooltip data key"
    }
  ],
  "stanza:menu-placement": "bottom-right",
  "stanza:style": [
    {
      "stanza:key": "--togostanza-series-0-color",
      "stanza:type": "color",
      "stanza:default": "#6590e6",
      "stanza:description": "Group color 0"
    },
    {
      "stanza:key": "--togostanza-series-1-color",
      "stanza:type": "color",
      "stanza:default": "#3ac9b6",
      "stanza:description": "Group color 1"
    },
    {
      "stanza:key": "--togostanza-series-2-color",
      "stanza:type": "color",
      "stanza:default": "#9ede2f",
      "stanza:description": "Group color 2"
    },
    {
      "stanza:key": "--togostanza-series-3-color",
      "stanza:type": "color",
      "stanza:default": "#F5DA64",
      "stanza:description": "Group color 3"
    },
    {
      "stanza:key": "--togostanza-series-4-color",
      "stanza:type": "color",
      "stanza:default": "#F57F5B",
      "stanza:description": "Group color 4"
    },
    {
      "stanza:key": "--togostanza-series-5-color",
      "stanza:type": "color",
      "stanza:default": "#F75976",
      "stanza:description": "Group color 5"
    },
    {
      "stanza:key": "--togostanza-node-label-font-family",
      "stanza:type": "text",
      "stanza:default": "Helvetica Neue",
      "stanza:description": "Font family"
    },
    {
      "stanza:key": "--togostanza-node-label-font-size",
      "stanza:type": "number",
      "stanza:default": 10,
      "stanza:description": "Label font size"
    },
    {
      "stanza:key": "--togostanza-node-label-font-color",
      "stanza:type": "color",
      "stanza:default": "#4E5059",
      "stanza:description": "Label font Color"
    },
    {
      "stanza:key": "--togostanza-node-label-outline-width",
      "stanza:type": "number",
      "stanza:default": 1,
      "stanza:description": "Line width"
    },
    {
      "stanza:key": "--togostanza-node-label-outline-opacity",
      "stanza:type": "number",
      "stanza:default": 0.6,
      "stanza:description": "Outline opacity"
    },
    {
      "stanza:key": "--togostanza-node-label-margin-y",
      "stanza:type": "number",
      "stanza:default": 20,
      "stanza:description": "Outline opacity"
    },
    {
      "stanza:key": "--togostanza-edge-label-background-color",
      "stanza:type": "color",
      "stanza:default": "#e6e6e6",
      "stanza:description": "Edge label rotation angle in degrees"
    },
    {
      "stanza:key": "--togostanza-edge-label-background-opacity",
      "stanza:type": "number",
      "stanza:default": 1,
      "stanza:description": "Edge label rotation angle in degrees"
    },
    {
      "stanza:key": "--togostanza-edge-label-rotation",
      "stanza:type": "number",
      "stanza:default": "Helvetica Neue",
      "stanza:description": "Edge label rotation angle in degrees"
    },
    {
      "stanza:key": "--togostanza-edge-label-font-family",
      "stanza:type": "text",
      "stanza:default": "Helvetica Neue",
      "stanza:description": "Edge label font family"
    },
    {
      "stanza:key": "--togostanza-edge-label-font-size",
      "stanza:type": "number",
      "stanza:default": 10,
      "stanza:description": "Edge label font size"
    },
    {
      "stanza:key": "--togostanza-edge-label-font-color",
      "stanza:type": "color",
      "stanza:default": "#4E5059",
      "stanza:description": "Edge label font Color"
    }
  ]
}
